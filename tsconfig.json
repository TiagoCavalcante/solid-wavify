{
  "compilerOptions": {
    "target": "ES6",
    "rootDir": "./src",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  }
}
